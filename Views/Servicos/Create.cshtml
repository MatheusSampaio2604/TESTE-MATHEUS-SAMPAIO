                <span asp-validation-for="Prazo_Entrega" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="Fornecedor" class="control-label"></label>
                <select asp-for="Fornecedor" class ="form-control" asp-items="ViewBag.Fornecedor"></select>
            </div>
            <div class="form-group">
                <input type="submit" value="Create" class="btn btn-primary" />
