@model TESTE_MATHEUS_SAMPAIO.Context.DTO.ServicosViewModel

@{
    ViewData["Title"] = "Create";
}

<h4>ServicosModel</h4>
<hr />
<div class="row">
    <div class="col-md-4">
        <form asp-action="Create">
            <div asp-validation-summary="ModelOnly" class="text-danger"></div>
